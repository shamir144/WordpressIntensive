			max: (typeof($fullPrice[0]) == 'undefined' || typeof($creditInitPaymentInput[0]) == 'undefined') ?
				Number($creditInitPaymentInput.attr('data-max') ? $creditInitPaymentInput.attr('data-max') : 1) :
				parseInt($fullPrice.val().replace(/\s/g,'')) * 0.9,
